entero fibonacci(entero var_numero) inicio

    entero resultado_fibonacci1;
    entero resultado_fibonacci2;

	si (var_numero igual 0) inicio
		retorno 0;
	fin

    si(var_numero igual 1) inicio
		retorno 1;
	fin sino inicio
	    resultado_fibonacci1 = fibonacci(var_numero - 1);
	    resultado_fibonacci2 = fibonacci(var_numero - 2);

		retorno resultado_fibonacci1 + resultado_fibonacci2;
	fin

fin

vacio principal() inicio

    entero numero = 10;
    entero resultado;

    resultado = fibonacci(numero);

fin